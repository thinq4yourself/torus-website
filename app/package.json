    "@walletconnect/iso-crypto": "^1.3.1",
    "assert": "^2.0.0",
    "await-semaphore": "^0.1.3",
    "bignumber.js": "^9.0.1",
    "bowser": "^2.11.0",
    "broadcast-channel": "^3.3.0",
    "clone": "^2.1.2",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "^3.8.0",
    "create-hash": "^1.2.0",
    "deepmerge": "^4.2.2",
    "eth-block-tracker": "^4.4.3",
    "eth-contract-metadata": "^1.16.0",
    "eth-json-rpc-filters": "^4.2.1",
