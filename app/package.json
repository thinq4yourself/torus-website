    "uglifyify": "^5.0.2",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0",
    "worker-loader": "^3.0.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:js --",
      "prettier --write 'src/**/*.js'"
    ],
    "*.vue": [
      "npm run lint:vue --",
      "prettier --write 'src/**/*.vue'"
    ],
    "*.{json,css,yaml,yml}": [
      "prettier --write '**/*.json'",
      "prettier --write '**/*.css'",
      "prettier --write '**/*.yaml'"
    ]
  },
  "mocha": {
    "timeout": 0
  }
}
