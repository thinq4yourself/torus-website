    "isomorphic-fetch": "^3.0.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.2",
    "luxon": "^1.25.0",
    "mocha": "^8.2.1",
    "mocha-eslint": "^6.0.0",
    "mocha-jsdom": "^2.0.0",
    "mocha-sinon": "^2.1.2",
    "nock": "13.0.5",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.0",
